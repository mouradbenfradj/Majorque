<!DOCTYPE html>
<html>
<head>
    <title>NewsFeed</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/assets/css/bootstrap.min.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/assets/css/font-awesome.min.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/assets/css/animate.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/assets/css/font.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/assets/css/li-scroller.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/assets/css/slick.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/assets/css/jquery.fancybox.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/assets/css/theme.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/assets/css/style.css') }}">
    <!--[if lt IE 9]>
    <script src="{{ asset('bundles/app/assets/js/html5shiv.min.js') }}"></script>
    <script src="{{ asset('bundles/app/assets/js/respond.min.js') }}"></script>
    <![endif]-->
</head>
<body>
<div id="preloader">
    <div id="status">&nbsp;</div>
</div>
<a class="scrollToTop" href="#"><i class="fa fa-angle-up"></i></a>
<div class="container">
    <header id="header">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="header_top">
                    <div class="header_top_left">
                        <ul class="top_nav">
                            <li><a href="{{ path('homepage') }}">Home</a></li>
                            {#<li><a href="#">About</a></li>#}
                            <li><a href="{{ path('contact') }}">Contact</a></li>
                        </ul>
                    </div>
                    <div class="header_top_right">
                        {% set nowDate = date('now') %}
                        <p>{{ nowDate|date('D, F d, Y') }}</p>
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="header_bottom">
                    <div class="logo_area"><a href="{{ path('homepage') }}" class="logo"><img
                                    src="{{ asset('bundles/app/images/majorque.png') }}" alt=""></a>
                    </div>
                    {#<div class="add_banner"><a href="#"><img src="images/addbanner_728x90_V1.jpg" alt=""></a></div>#}
                </div>
            </div>
        </div>
    </header>
    {% block body %}

    {% endblock %}

</div>
<script src="{{ asset('bundles/app/assets/js/jquery.min.js') }}"></script>
<script src="{{ asset('bundles/app/assets/js/wow.min.js') }}"></script>
<script src="{{ asset('bundles/app/assets/js/bootstrap.min.js') }}"></script>
<script src="{{ asset('bundles/app/assets/js/slick.min.js') }}"></script>
<script src="{{ asset('bundles/app/assets/js/jquery.li-scroller.1.0.js') }}"></script>
<script src="{{ asset('bundles/app/assets/js/jquery.newsTicker.min.js') }}"></script>
<script src="{{ asset('bundles/app/assets/js/jquery.fancybox.pack.js') }}"></script>
<script src="{{ asset('bundles/app/assets/js/custom.js') }}"></script>
</body>
</html>